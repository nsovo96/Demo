@model IEnumerable<DataAccessLayer.Models.Users>

@{
    ViewBag.Title = "DisplayEmployee";
    Layout = "~/Views/Shared/" + Session["addtoLayout"].ToString();
}

<h2>DisplayEmployee</h2>

<p>
    @Html.ActionLink("Add employee", "Register")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FullNames)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserRole)
        </th>
   
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FullNames)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserRole)
        </td>

    </tr>
}

</table>
